        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prettier": "prettier --write .",
        "resources": "npx capacitor-assets generate --android --ios",
        "sync": "yarn cap sync",
        "updateMainActivityAndroid": "node scripts/updateMainActivity.js"
    },
