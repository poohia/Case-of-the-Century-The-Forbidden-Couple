    "scripts": {
        "start": "vite --host",
        "build-react": "cross-env NODE_ENV=production npm-run-all --serial tsc vite-build",
        "build": "cross-env NODE_ENV=production npm-run-all --serial tsc vite-build resources trapeze updateMainActivityAndroid sync",
