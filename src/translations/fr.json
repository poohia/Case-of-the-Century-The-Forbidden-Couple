    },
    {
        "key": "retrospaceadventure_fight_action_bd_bull_text_2",
        "text": "Un moustique m'attaque???",
        "editable": true,
        "deletable": true
    },
    {
        "key": "retrospaceadventure_fight_action_bd_bull_text_3",
        "text": "Je suis perplexe",
        "editable": true,
        "deletable": true
    },
    {
        "key": "speech_bubble_23",
        "text": "Le plus grand, le plus beau, le plus fort, R5-D4-GPT",
        "editable": true,
        "deletable": true
    },
    {
        "key": "speech_bubble_24",
        "text": "Permettez-moi d'en douter",
        "editable": true,
        "deletable": true
    },
    {
        "key": "speech_bubble_25",
        "text": "Ne vous laissez pas avoir par les apparences",
        "editable": true,
        "deletable": true
    },
    {
        "key": "speech_bubble_26",
        "text": "Voyons ce qu'il vaut en combat",
        "editable": true,
        "deletable": true
    },
    {
        "key": "speech_bubble_27",
        "text": "Erreur système, erreeeeeeeeeeeur",
        "editable": true,
        "deletable": true
    },
